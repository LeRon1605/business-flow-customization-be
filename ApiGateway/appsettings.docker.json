{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "hub",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/hub/{everything}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET",
        "DELETE"
      ],
      "SwaggerKey": "hub"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "identity",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/identity/{everything}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET",
        "DELETE"
      ],
      "SwaggerKey": "identity"
    },
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "business-flow",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/business-flow/{everything}",
      "UpstreamHttpMethod": [
        "POST",
        "PUT",
        "GET",
        "DELETE"
      ],
      "SwaggerKey": "business-flow"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "hub",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Hub",
          "Version": "1.0",
          "Url": "http://hub/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "identity",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Identity",
          "Version": "1.0",
          "Url": "http://identity/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "business-flow",
      "TransformByOcelotConfig": true,
      "Config": [
        {
          "Name": "Business Flow",
          "Version": "1.0",
          "Url": "http://business-flow/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
